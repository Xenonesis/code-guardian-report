{
    "name": "code-guardian-report",
    "private": true,
    "version": "9.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build": "tsc --noEmit && vite build",
        "build:production": "cross-env NODE_ENV=production npm run build",
        "build:analyze": "cross-env ANALYZE=true vite build",
        "preview": "vite preview --host",
        "type-check": "tsc --noEmit",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
        "start": "npm run dev",
        "serve": "npm run preview",
        "clean": "rimraf dist node_modules/.vite",
        "prepare": "node scripts/setup-git-hooks.js",
        "prebuild": "npm run type-check",
        "postbuild": "echo Build completed successfully!",
        "update-contributors": "node scripts/update-contributors.js",
        "setup-git-hooks": "node scripts/setup-git-hooks.js",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "test-automation": "node scripts/test-automation.js",
        "test:e2e-zip": "tsx scripts/e2e-zip-analysis.ts",
        "test:multi-language": "tsx src/tests/multiLanguageAnalysis.test.ts",
        "test:multi-language-auto": "node scripts/run-multi-language-tests.js",
        "ci:multi-language-check": "node scripts/ci-multi-language-check.js",
        "size": "size-limit",
        "size:analyze": "size-limit --why",
        "lighthouse": "lhci autorun",
        "lighthouse:collect": "lhci collect",
        "lighthouse:assert": "lhci assert"
    },
    "dependencies": {
        "@babel/parser": "^7.28.5",
        "@babel/traverse": "^7.28.5",
        "@babel/types": "^7.28.5",
        "@lezer/cpp": "^1.1.3",
        "@lezer/java": "^1.1.3",
        "@lezer/python": "^1.1.18",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-collapsible": "^1.1.12",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-label": "^2.1.8",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-progress": "^1.1.8",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-switch": "^1.2.6",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-toast": "^1.2.15",
        "@radix-ui/react-tooltip": "^1.2.8",
        "@tailwindcss/postcss": "^4.1.17",
        "@vercel/analytics": "^1.5.0",
        "@vercel/speed-insights": "^1.2.0",
        "@vitejs/plugin-react-swc": "^4.2.2",
        "acorn": "^8.15.0",
        "acorn-walk": "^8.3.4",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "esprima": "^4.0.1",
        "esquery": "^1.6.0",
        "firebase": "^12.6.0",
        "framer-motion": "^12.23.24",
        "html2canvas": "^1.4.1",
        "jspdf": "^3.0.4",
        "jszip": "^3.10.1",
        "lenis": "^1.3.15",
        "lucide-react": "^0.555.0",
        "next-themes": "^0.4.6",
        "postcss": "^8.5.6",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "recharts": "^3.5.1",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.17",
        "typescript": "^5.9.3",
        "vite": "^7.2.4",
        "web-tree-sitter": "^0.25.10",
        "web-vitals": "^5.1.0"
    },
    "devDependencies": {
        "@eslint/markdown": "^7.5.1",
        "@size-limit/preset-app": "^11.2.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "happy-dom": "^20.0.11",
        "rimraf": "^6.0.1",
        "size-limit": "^11.2.0",
        "terser": "^5.44.1",
        "tsx": "^4.20.6",
        "vitest": "^4.0.14"
    },
    "size-limit": [
        {
            "path": "dist/assets/js/*.js",
            "limit": "1.2 MB"
        }
    ],
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
