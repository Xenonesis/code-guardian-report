  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://code-guardian-report.vercel.app/"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 420
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "file_handlers": [
    {
      "action": "/analyze",
      "accept": {
        "application/zip": [".zip"],
        "application/x-tar": [".tar"],
        "application/gzip": [".gz", ".tar.gz"],
        "text/javascript": [".js"],
        "text/typescript": [".ts"],
        "text/x-python": [".py"],
        "text/x-java-source": [".java"],
        "text/x-c++src": [".cpp", ".cc", ".cxx"],
        "text/x-go": [".go"],
        "text/x-rust": [".rs"]
      },
      "launch_type": "single-client"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+codeguardian",
      "url": "/analyze?url=%s"
    }
  ],
  "share_target": {
    "action": "/share-analyze",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "codeFiles",
          "accept": [
            "application/zip",
            "text/javascript",
            "text/typescript",
            "text/x-python",
            "text/x-java-source"
          ]
        }
      ]
    }
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "scope_extensions": [
    {
      "origin": "https://code-guardian-report.vercel.app"
    }
  ],
  "id": "code-guardian-enterprise",
  
  "background_sync": ["file-upload", "analysis-data", "user-preferences", "offline-sync", "analytics-data"],
  "offline_enabled": true,
  "cache_strategy": "network-first-cache-fallback",
  "permissions": [
    "background-sync",
    "push",
    "notifications",
    "storage",
    "camera",
    "microphone",
    "persistent-storage"
  ],
  "features": [
    "background-sync",
    "push-notifications",
    "offline-analytics",
    "file-handling",
    "share-target",
    "protocol-handlers",
    "shortcuts",
    "window-controls-overlay"
  ],
  "performance": {
    "cache_first_assets": [".js", ".css", ".woff2", ".png", ".jpg", ".svg"],
    "network_first_routes": ["/api/", "/auth/"],
    "preload_routes": ["/", "/analyze", "/reports"],
    "max_cache_size": "100MB",
    "cache_expiry": "30d"
  },
  "analytics": {
    "track_installs": true,
    "track_usage": true,
    "track_performance": true,
    "track_offline": true,
    "privacy_compliant": true
  }
}
